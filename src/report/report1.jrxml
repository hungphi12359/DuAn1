	<parameter name="MaHoaDon" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[11]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     HoaDon."MaHoaDon" AS HoaDon_MaHoaDon,
     HoaDon."SoHoaDon" AS HoaDon_SoHoaDon,
     HoaDon."NgayTaoHD" AS HoaDon_NgayTaoHD,
     HoaDon."MaNhanVien" AS HoaDon_MaNhanVien,
     HoaDon."MaSinhVien" AS HoaDon_MaSinhVien,
     HoaDon."MaChuyenNganhSV" AS HoaDon_MaChuyenNganhSV,
     HoaDon."HocPhi" AS HoaDon_HocPhi,
     HoaDon."TinhTrang" AS HoaDon_TinhTrang,
     HoaDon."TongTien" AS HoaDon_TongTien,
     HoaDon."HocKy" AS HoaDon_HocKy,
     HocKy."MaHocKy" AS HocKy_MaHocKy,
     NhanVien."MaNhanVien" AS NhanVien_MaNhanVien,
     SinhVien."MaSinhVien" AS SinhVien_MaSinhVien
FROM
     "dbo"."HocKy" HocKy INNER JOIN "dbo"."HoaDon" HoaDon ON HocKy."MaHocKy" = HoaDon."HocKy"
     INNER JOIN "dbo"."NhanVien" NhanVien ON HoaDon."MaNhanVien" = NhanVien."MaNhanVien"
     INNER JOIN "dbo"."SinhVien" SinhVien ON HoaDon."MaSinhVien" = SinhVien."MaSinhVien"
WHERE
     HoaDon."MaHoaDon" = $P{MaHoaDon}]]>
	</queryString>
